// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("CLEARDB_DATABASE_URL")
}

model Endereco {
  id Int @id @default(autoincrement())
  nome String
  rua String
}
